dialog(name: '$$$/AdobePlugin/Shared/LensCorrect/Process/Name=Lens Correction'){	group(placement: place_row)	{		group(placement: place_column)		{			cluster(child_horizontal: align_fill, horizontal: align_fill, name: '$$$/AdobePlugin/Shared/LensCorrect/FileSelectDialog/SourceFiles=Source Files')			{				group(margin_top: 4, placement: place_column)				{					static_text(horizontal: align_left, max_width: 420, multiline: false, name: '$$$/AdobePlugin/Shared/LensCorrectDialog/ChooseFiles=Choose files for batch lens correction.', vertical: align_center, view_id: '_intro', width: 320);					group(horizontal: align_left, placement: place_row)					{						group(margin_top: 3, placement: place_column, vertical: align_top)						{							static_text(name: '$$$/AdobePlugin/Shared/LensCorrect/Use=Use:', vertical: align_bottom);						}						group(horizontal: align_left, placement: place_column)						{							popup_menu(horizontal: align_left, resource_id: 100, view_id: '_source');							group(horizontal: align_left, placement: place_row)							{								listbox(height: 152, horizontal: align_left, item_id: 'multiselect: true', view_id: '_fileList', width: 354);								group(horizontal: align_left, placement: place_column, vertical: align_top)								{									button(horizontal: align_fill, name: '$$$/AdobePlugin/Shared/LensCorrect/Browse=&Browse...', view_id: '_browse');									button(horizontal: align_fill, name: '$$$/AdobePlugin/Shared/LensCorrect/Remove=&Remove', view_id: '_remove');									button(name: '$$$/AdobePlugin/Shared/LensCorrect/AddOpen=Add Open &Files', view_id: '_addOpenDocs');								}							}						}					}				}			}			cluster(horizontal: align_fill, name: '$$$/AdobePlugin/Shared/LensCorrect/Destination=Destination Folder')			{				group(horizontal: align_fill, placement: place_column)				{					group(placement: place_row, horizontal:align_left)					{						static_text(name: '$$$/AdobePlugin/Shared/LensCorrect/FileType=File Type:');						popup_menu(resource_id: 200, view_id:'_filetypes');					}					group(horizontal: align_fill, placement: place_row)					{						button(name: '$$$/AdobePlugin/Shared/LensCorrect/DestChoose=C&hoose...', view_id: '_choosedest');						static_text(horizontal: align_fill, vertical:align_bottom, view_id: '_destination', width:420, text_trim: 'truncate: middle');					}				}			}			cluster(child_horizontal: align_fill, horizontal: align_fill, name: '$$$/AdobePlugin/Shared/LensCorrect/LCPFile=Lens Correction Profile')			{				group(horizontal: align_fill, placement: place_column)				{					checkbox(name: '$$$/AdobePlugin/Shared/LensCorrect/AutoMatchLCP=&Match Best Profile', view_id: '_automatch', horizontal: align_left);					group(horizontal: align_fill, placement: place_row)					{						button(name: '$$$/AdobePlugin/Shared/LensCorrect/Choose=Ch&oose...', view_id: '_chooselcp');						static_text(horizontal: align_fill, vertical:align_bottom, view_id: '_lcpFile', width:420);					}				}			}			cluster(horizontal: align_fill, name: '$$$/AdobePlugin/Shared/LensCorrect/Options=Correction Options')			{            group(placement: place_row, horizontal: align_fill, spacing: 20)            {                group(horizontal: align_fill, placement: place_column)                {                    checkbox(name: '$$$/LensCorrect/Dialog/AutoCorrectGeometricDistortion=Geometric &Distortion', view_id: '_correctGD', horizontal: align_left);                    checkbox(name: '$$$/LensCorrect/Dialog/AutoCorrectChromaticAberration=&Chromatic Aberration', view_id: '_correctCA', horizontal: align_left);                    checkbox(name: '$$$/LensCorrect/Dialog/AutoVignetteRemoval=&Vignette', view_id: '_vignette', horizontal: align_left);                }                group(horizontal: align_fill, placement: place_column)                {                    checkbox(name: '$$$/LensCorrect/Dialog/AutoScaleImage=Auto &Scale Image', view_id: '_autoScale', horizontal: align_left);                    group(placement: place_row, horizontal:align_left)                    {                        static_text(name: '$$$/LensCorrect/Dialog/EdgeFillTypePopup=&Edge:');                        popup_menu(horizontal: align_left, resource_id: 300, view_id: '_edgetypes');                    }                }            }			}		}		group(horizontal: align_left, margin_top: 5, placement: place_column, vertical: align_top)		{			button(horizontal: align_fill, item_id: 'name: ok', name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/OK=OK', view_id: '_ok');			button(horizontal: align_fill, item_id: 'name: cancel', name: '$$$/AdobePlugin/Shared/Exposuremerge/FileSelectDialog/Cancel=Cancel', view_id: '_cancel');		}	}}